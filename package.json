{
  "version": "0.0.0",
  "name": "project-template",
  "scripts": {
    "dev": "vite",
    "dev:test": "vite --mode test",
    "dev:prod": "vite --mode prod",
    "lint": "concurrently \"npm:lint:eslint\" \"npm:lint:typescript\"",
    "lint:eslint": "eslint 'src/**/*' --fix",
    "lint:typescript": "concurrently \"tsc -p tsconfig.json --noEmit --pretty --skipLibCheck\" \"npx vue-tsc --noEmit --skipLibCheck\"",
    "prettier": "prettier --write '**/*.{vue,jsx,tsx,js,ts,json,md}'",
    "prepare": "husky install",
    "lint-staged": "lint-staged",
    "build": "npm run lint && vite build",
    "build:test": "npm run lint && vite build --mode test",
    "build:prod": "npm run lint && vite build --mode prod",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest",
    "serve": "vite preview"
  },
  "dependencies": {
    "@vueuse/core": "^6.4.1",
    "axios": "^0.21.1",
    "dayjs": "^1.10.6",
    "kurimudb": "5.0.2",
    "kurimudb-driver-cookie": "5.0.1",
    "kurimudb-driver-localstorage": "5.0.1",
    "less": "^4.1.1",
    "mockjs": "^1.1.0",
    "pinia": "^2.0.0-rc.4",
    "query-string": "^7.0.1",
    "vue": "^3.2.12",
    "vue-router": "^4.0.11"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^4.29.2",
    "@typescript-eslint/parser": "^4.29.2",
    "@vitejs/plugin-legacy": "^1.5.1",
    "@vitejs/plugin-vue": "^1.4.0",
    "@vitejs/plugin-vue-jsx": "^1.1.7",
    "@vue/compiler-sfc": "^3.2.12",
    "babel-eslint": "^10.1.0",
    "concurrently": "^6.3.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.24.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-vue": "^7.18.0",
    "husky": "^7.0.1",
    "lint-staged": "^11.1.2",
    "prettier": "^2.4.1",
    "sass": "^1.38.0",
    "typescript": "^4.5.2",
    "unplugin-auto-import": "^0.4.13",
    "unplugin-vue-components": "^0.16.0",
    "vite": "^2.7.7",
    "vite-plugin-compression": "^0.3.5",
    "vite-plugin-mock": "^2.9.6",
    "vite-plugin-pages": "^0.19.0-beta.6",
    "vite-plugin-style-import": "^1.4.0",
    "vite-plugin-vue-layouts": "^0.4.1",
    "vite-plugin-windicss": "^1.4.12",
    "vitest": "^0.0.115",
    "vue-eslint-parser": "^8.0.1",
    "vue-tsc": "^0.29.3",
    "windicss": "^3.1.9"
  }
}
