{
  "version": "0.0.0",
  "name": "project-template",
  "scripts": {
    "dev": "vite",
    "dev:test": "vite --mode test",
    "dev:prod": "vite --mode prod",
    "lint": "concurrently \"npm:lint:eslint\" \"npm:lint:typescript\"",
    "lint:eslint": "eslint 'src/**/*' --fix",
    "lint:typescript": "concurrently \"tsc -p tsconfig.json --noEmit --pretty --skipLibCheck\"",
    "prettier": "prettier --write '**/*.{jsx,tsx,js,ts,json,md}'",
    "prepare": "husky install",
    "lint-staged": "lint-staged",
    "build": "npm run lint && vite build",
    "build:test": "npm run lint && vite build --mode test",
    "build:prod": "npm run lint && vite build --mode prod",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest",
    "serve": "vite preview"
  },
  "dependencies": {
    "@vitejs/plugin-react": "^1.1.2",
    "axios": "^0.21.1",
    "dayjs": "^1.10.6",
    "kurimudb": "5.0.2",
    "kurimudb-driver-cookie": "5.0.1",
    "kurimudb-driver-localstorage": "5.0.1",
    "less": "^4.1.1",
    "mockjs": "^1.1.0",
    "query-string": "^7.0.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^6.1.1"
  },
  "devDependencies": {
    "@types/jest": "^27.0.3",
    "@types/react": "^17.0.37",
    "@types/react-dom": "^17.0.11",
    "@typescript-eslint/eslint-plugin": "^4.29.2",
    "@typescript-eslint/parser": "^4.29.2",
    "@vitejs/plugin-legacy": "^1.5.1",
    "antd": "^4.17.3",
    "babel-eslint": "^10.1.0",
    "concurrently": "^6.3.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.24.0",
    "eslint-plugin-prettier": "^3.4.0",
    "husky": "^7.0.1",
    "jest": "^27.3.1",
    "lint-staged": "^11.1.2",
    "prettier": "^2.4.1",
    "ts-jest": "^27.0.7",
    "typescript": "^4.5.2",
    "vite": "^2.5.0",
    "vite-plugin-compression": "^0.3.5",
    "vite-plugin-mock": "^2.9.6",
    "vite-plugin-pages": "^0.19.0-beta.6",
    "vite-plugin-style-import": "^1.4.0",
    "vite-plugin-windicss": "^1.4.12",
    "windicss": "^3.1.9"
  }
}
