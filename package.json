{
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "tsc": "vue-tsc --noEmit",
    "lint": "eslint .  --ext .js",
    "prettier": "prettier --write '**/*.{js,ts,json,md}'",
    "build": "npm run tsc && vite build",
    "serve": "vite preview"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.js": [
      "prettier --write",
      "npm run tsc",
      "git add"
    ]
  },
  "dependencies": {
    "@ant-design/icons-vue": "^6.0.1",
    "@vueuse/core": "^6.0.0",
    "ant-design-vue": "^2.2.6",
    "axios": "^0.21.1",
    "dayjs": "^1.10.6",
    "js-cookie": "^2.2.1",
    "postcss-px-to-viewport": "^1.1.1",
    "vite-plugin-compression": "^0.2.6",
    "vue": "^3.2.2",
    "vue-router": "^4.0.11",
    "vuex": "^4.0.2",
    "zigg-request-sign": "^1.0.2"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^1.4.0",
    "@vue/compiler-sfc": "^3.2.2",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.32.0",
    "eslint-config-egg": "^9.0.0",
    "husky": "^7.0.1",
    "lint-staged": "^11.1.2",
    "prettier": "^2.3.2",
    "sass": "^1.37.5",
    "typescript": "^4.3.5",
    "vite": "^2.4.4",
    "vite-plugin-pwa": "^0.7.3",
    "vue-tsc": "^0.0.24"
  }
}
