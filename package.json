{
  "version": "0.0.0",
  "name": "project-template",
  "scripts": {
    "dev": "vite",
    "dev:test": "vite --mode test",
    "dev:prod": "vite --mode prod",
    "lint": "eslint 'src/**/*' --fix",
    "prettier": "prettier --write '**/*.{js,ts,json,md}'",
    "prepare": "husky install",
    "lint-staged": "lint-staged",
    "build": "npm run lint && vite build",
    "build:test": "npm run lint && vite build --mode test",
    "build:prod": "npm run lint && vite build --mode prod",
    "serve": "vite preview"
  },
  "lint-staged": {
    "*.{vue,ts,tsx,js,jsx,json,markdown}": [
      "prettier --write",
      "eslint --fix",
      "git add"
    ]
  },
  "dependencies": {
    "@ant-design/icons-vue": "^6.0.1",
    "@vueuse/core": "^6.0.0",
    "ant-design-vue": "^2.2.6",
    "axios": "^0.21.1",
    "dayjs": "^1.10.6",
    "kurimudb": "^4.1.1",
    "kurimudb-driver-cookie": "^4.0.0",
    "kurimudb-driver-localstorage": "^4.0.0",
    "less": "^4.1.1",
    "mockjs": "^1.1.0",
    "normalize.css": "^8.0.1",
    "pinia": "^2.0.0-rc.4",
    "postcss-px-to-viewport": "^1.1.1",
    "query-string": "^7.0.1",
    "sass-utils": "^1.0.56",
    "vue": "^3.2.4",
    "vue-router": "^4.0.11"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^4.29.2",
    "@typescript-eslint/parser": "^4.29.2",
    "@vitejs/plugin-legacy": "^1.5.1",
    "@vitejs/plugin-vue": "^1.4.0",
    "@vitejs/plugin-vue-jsx": "^1.1.7",
    "@vue/compiler-sfc": "^3.2.4",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.24.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-vue": "^7.17.0",
    "husky": "^7.0.1",
    "lint-staged": "^11.1.2",
    "prettier": "^2.3.2",
    "sass": "^1.38.0",
    "typescript": "^4.3.5",
    "vite": "^2.5.0",
    "vite-plugin-components": "^0.13.2",
    "vite-plugin-compression": "^0.3.5",
    "vite-plugin-mock": "^2.9.6",
    "vite-plugin-pages": "^0.18.0",
    "vite-plugin-pwa": "^0.7.3",
    "vite-plugin-style-import": "^1.2.1",
    "vite-plugin-vue-layouts": "^0.4.1",
    "vue-tsc": "^0.0.24"
  }
}
